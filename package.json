{
  "name": "premium-jewelry-website",
  "version": "1.0.0",
  "description": "Premium jewelry e-commerce website with React frontend and FastAPI backend",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && python -m uvicorn server:app --host 0.0.0.0 --port 8000 --reload",
    "frontend": "cd frontend && npm start",
    "install-all": "npm install && cd frontend && npm install && cd ../backend && python3 -m pip install -r requirements.txt",
    "build": "cd frontend && npm run build",
    "test": "cd backend && python backend_test.py",
    "start": "npm run dev"
  },
  "keywords": [
    "jewelry",
    "e-commerce",
    "react",
    "fastapi",
    "mongodb",
    "luxury"
  ],
  "author": "LUXE Admin",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luxejewelry/premium-jewelry-website"
  }
}